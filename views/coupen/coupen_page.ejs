<div class=" tm-block-col">
    <div class="tm-bg-primary-dark tm-block tm-block-taller tm-block-scroll">
        <h1 style="color: #ffff;
    text-align: center;
    font-size: revert;">Available coupons</h1>
    <table class="table" >
        <thead>
          <tr>
            <th>Code</th>
            <th>Discount</th>
            <th>Min_Purchase</th>
            <th>Max_Discount</th>
            <th>Expiration_Date</th>
          </tr>
        </thead>
        <tbody>
          <% if (coupen.length > 0) { %>
            <% coupen.forEach(c => { %>
              <tr style="color: #fff">
                <td><%= c.code %></td>
                <td><%= c.discount %></td>
                <td><%= c.min_Purchase %></td>
                <td><%= c.max_Discount %></td>
                <td><%= c.expirationDate.toLocaleDateString() %></td>
              </tr>
            <% }); %>
          <% } else { %>
            <tr>
              <td colspan="5">Coupons Not Found</td>
            </tr>
          <% } %>
        </tbody>
      </table>


    </div>
